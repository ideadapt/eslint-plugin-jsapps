{
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "brace-style": [
            2,
            "1tbs"
        ],
        "default-case": 2,
        "func-style": [
            2,
            "declaration"
        ],
        "guard-for-in": 0,
        "no-floating-decimal": 2,
        "no-nested-ternary": 2,
        "no-undefined": 2,
        "radix": 2,
        "keyword-spacing": [
            2,
            {
                "before": true,
                "after": true
            }
        ],
        "no-multi-spaces": 0,
        "valid-jsdoc": [
            2,
            {
                "prefer": {
                    "return": "returns"
                }
            }
        ],
        "wrap-iife": 2
    }
}
